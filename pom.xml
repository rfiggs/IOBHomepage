	<groupId>edu.hawaii.its</groupId>
	<artifactId>EmployeeIOB</artifactId>
	<packaging>war</packaging>
	<version>0.0.1-SNAPSHOT</version>
	<name>EmployeeIOB</name>
	<url>http://maven.apache.org</url>

	<properties>

